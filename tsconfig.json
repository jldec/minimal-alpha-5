{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["DOM", "DOM.Iterable", "ESNext", "ES2022"],
    "jsx": "react-jsx",
    "module": "es2022",
    "moduleResolution": "bundler",
    "types": ["node", "@cloudflare/workers-types/2023-07-01", "./worker-configuration.d.ts"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "checkJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
