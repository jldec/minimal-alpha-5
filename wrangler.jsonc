{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "minimal-alpha-5",
  "main": "src/worker.tsx",
  "compatibility_date": "2025-05-08",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "ai": { "binding": "AI" },
  "vars": {
    "CHAT_AGENT_PATH": "chat-agent", // matches binding-name, duplicated in Agent.tsx
    "CHAT_AGENT_NAME": "agent", // duplicated in Agent.tsx
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "CHAT_AGENT",
        "class_name": "Chat"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Chat"]
    }
  ]
}
